<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<HBox prefHeight="371.0" prefWidth="756.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <VBox prefHeight="371.0" prefWidth="453.0">
        <TableView prefHeight="263.0" prefWidth="674.0" fx:id="personTableView">
            <columns>
            </columns>
        </TableView>
        <HBox alignment="CENTER" prefHeight="90.0" prefWidth="525.0">
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Previous" fx:id="prevPersonButton">
                <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Next"
                    textAlignment="CENTER" fx:id="nextPersonButton">
                <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
            </Button>
        </HBox>
    </VBox>
    <VBox alignment="CENTER" prefHeight="371.0" prefWidth="286.0" spacing="10.0">
        <HBox.margin>
            <Insets/>
        </HBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <!-- Input fields -->
        <TextField fx:id="personIdField" text="ID" editable="false"/>
        <TextField fx:id="personUsernameField" promptText="Username"/>
        <TextField fx:id="personEmailField" promptText="Email"/>
        <TextField fx:id="personPasswordField" promptText="Password" />
        <TextField fx:id="personFirstName" promptText="First name"/>
        <TextField fx:id="personLastName" promptText="Last name"/>
        <TextField fx:id="personOccupationField" promptText="Occupation"/>
        <TextField fx:id="personEmpathyScoreField" promptText="EmpathyScore"/>

        <!-- Action buttons -->
        <Button fx:id="personAddButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="150.0" text="Add"/>
        <Button fx:id="personRemoveButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="150.0" text="Remove" disable="false"/>

    </VBox>
</HBox>
