<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox prefHeight="200.0" prefWidth="100.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <HBox prefHeight="210.0" prefWidth="600.0">
        <TableView prefHeight="210.0" prefWidth="450.0" fx:id="eventTableView">
            <columns>
            </columns>
        </TableView>
        <TableView prefHeight="210.0" prefWidth="180.0" fx:id="subscribersTableView">
            <columns>
            </columns>
        </TableView>
    </HBox>
    <HBox alignment="CENTER" prefHeight="163.0" prefWidth="751.0">
        <VBox alignment="CENTER" prefHeight="162.0" prefWidth="498.0">
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Notify subscribers" disable="true"
                fx:id="notifySubsButton">
                <VBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </VBox.margin>
            </Button>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="498.0">
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Add Event" fx:id="addEventButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Remove Event" disable="true" fx:id="removeEventButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="498.0">
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Add Subscriber" fx:id="addSubButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                        text="Remove Subscriber" disable="true" fx:id="removeSubButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="498.0">
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Previous"
                    fx:id="previousEventButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Next" textAlignment="CENTER"
                    fx:id="nextEventButton">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </HBox>
        </VBox>
        <VBox alignment="CENTER" prefHeight="162.0" prefWidth="202.0">
            <TextField prefHeight="30.0" promptText="ID" fx:id="eventIdTextField">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </TextField>
            <TextField prefHeight="30.0" promptText="Name/User ID" fx:id="eventNameTextField">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </TextField>
        </VBox>
    </HBox>
</VBox>
