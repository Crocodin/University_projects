<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox prefHeight="371.0" prefWidth="756.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <TableView prefHeight="263.0" prefWidth="674.0" fx:id="friendTableView">
        <columns>
        </columns>
    </TableView>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <TextField fx:id="userOneId" promptText="User One Id">
            <HBox.margin>
                <Insets right="10.0"/>
            </HBox.margin>
        </TextField>
        <TextField fx:id="userTwoId" promptText="User Two Id">
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </TextField>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <Button mnemonicParsing="false" prefWidth="200.0" text="Remove Friendship" fx:id="removeFriendButton">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" prefWidth="200.0" text="Add FriendShip" fx:id="addFriendButton">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Previous" fx:id="prevFriendButton">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Next"
                textAlignment="CENTER" fx:id="nextFriendButton">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</VBox>
